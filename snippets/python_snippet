		"body": [
			"def $1(l,n):",
			"  pass",
			"",
			"t=int(input())",
			"for _ in range(t):",
			"  n=int(input())",
			"  a,b=map(int,input().split())",
			"  l=list(map(int,input().split()))",
			"  print($1(l,n))"
		],
		"description": "Test Cases input snippet for CP"
